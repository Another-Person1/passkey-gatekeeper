{
    "name": "Passkey Blocker",
    "description": "Restricts passkey prompts on websites you choose",
    "version": "1.0",
    "manifest_version": 3,

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_start"
        }
    ]
}